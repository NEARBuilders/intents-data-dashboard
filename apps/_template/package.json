    "@orpc/tanstack-query": "^1.11.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@walletconnect/sign-client": "^2.23.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.1.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@module-federation/enhanced": "^0.8.12",
    "@module-federation/rsbuild-plugin": "^0.8.12",
    "@rsbuild/core": "^1.6.6",
