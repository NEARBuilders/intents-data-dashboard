    "@tanstack/router-plugin": "^1.136.5",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "near-api-js": "^5.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18"
