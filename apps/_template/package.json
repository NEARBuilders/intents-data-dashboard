{
  "name": "@efiz.near/profile",
  "version": "0.0.1",
  "homepage": "/",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cat welcome.txt && rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "fmt": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "fmt:check": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "deploy": "bun run --bun scripts/deploy.ts"
