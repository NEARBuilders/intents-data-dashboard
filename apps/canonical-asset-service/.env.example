ASSET_ENRICHMENT_REMOTE_ENTRY_URL=http://localhost:3010

# Aggregator URL for orchestrated sync
AGGREGATOR_URL=http://localhost:8787/api/rpc

# Optional: Cron schedule for automatic sync (in UTC)
# Examples:
#   "0 0 * * 0"     - Weekly on Sunday at midnight UTC
#   "0 0 1 * *"     - Monthly on the 1st at midnight UTC
#   "0 2 * * *"     - Daily at 2 AM UTC
# Leave empty to disable automatic sync
SYNC_CRON_SCHEDULE="0 0 1 * *"
